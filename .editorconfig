# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{css,scss,sass}]
charset = utf-8
indent_style = tab
indent_size = unset
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,ts,jsx,tsx}]
charset = utf-8
indent_style = space
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
